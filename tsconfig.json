{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "lib": ["es6"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
